	border-right-color: #b3bdc4 !important;
}

.sys-setting-arrow{
	background-color: #71cb35;
	padding:9px 0;
	font-size: 14px;
	color: #fff;
	position: relative;
}

.sys-setting-arrow.gray,
.sys-arrow-holder .sys-box.gray{
	background-color: #79797b;
}

.sys-setting-arrow::after{
	content:'';
	display: block;
	width: 0;
	height: 0;
	border:19px solid transparent;
	border-left-color:#71cb35;
	position: absolute;
	right: -37px;
	top: 0;
	z-index: 999;
}

.sys-setting-arrow.gray::after{
	border-left-color:#79797b !important;
}

.sys-setting-arrow:first-child{
	-webkit-border-top-left-radius: 2px;
	-moz-border-top-left-radius: 2px;
	border-top-left-radius: 2px;

	-webkit-border-bottom-left-radius: 2px;
	-moz-border-bottom-left-radius: 2px;
	border-bottom-left-radius: 2px;
}

.sys-setting-arrow:last-child{
	-webkit-border-top-right-radius: 2px;
	-moz-border-top-right-radius: 2px;
	border-top-right-radius: 2px;

	-webkit-border-bottom-right-radius: 2px;
	-moz-border-bottom-right-radius: 2px;
	border-bottom-right-radius: 2px;
}

.sys-arrow-holder{
	position: relative;
	padding-top: 40px;
}

.sys-arrow-holder::before,
.sys-arrow-holder::after{
	content: '';
	display: block;
	width: 0;
	position: absolute;
}

.sys-arrow-holder::before{
	height: 40%;
	border:1.5px solid #79797b;
	top: -13%;
	left: 47.8%;
}

.sys-arrow-holder::after{
	height: 0;
	left: 45.6%;
	top: 27%;
	border:7px solid transparent;
	border-top-color: #79797b;
	z-index: 999;
}

.sys-arrow-holder .sys-box{
	width: 210px;
	height: 74px;
	background-color: #71cb35;
	margin: 0 auto;
	border-radius: 2px;
	position: relative;
}

.sys-arrow-holder .sys-box span{
	font-size: 14px;
	color: #fff;
	width: 60%;
	height: 30px;
	position: absolute;
	left: 0;
	top: 0;
	bottom: 0;
	right: 0;
	margin: auto;
	text-align: center;
}

