.faq-list ol,
.faq-list ol li{
	list-style: none;
	list-style-type: none;
	padding: 0;
	margin:0; 
}

.faq-list ol li{
	padding:10px 0 10px 80px;
	position: relative;
}

.faq-list ol li a{
	position: relative;
	display: block;
	width: 100%;
	padding:12px 15px;
	background: #a4a4a4;
	color: #222;
	font-size: 14px;
	-webkit-border-top-right-radius: 2px;
	-moz-border-top-right-radius: 2px;
	-ms-border-top-right-radius: 2px;
	border-top-right-radius: 2px;

	-webkit-border-bottom-right-radius: 2px;
	-moz-border-bottom-right-radius: 2px;
	-ms-border-bottom-right-radius: 2px;
	border-bottom-right-radius: 2px;

	transition: all ease 0.3s;
}

.faq-list ol li a::before{
	content: '';
	width: 0;
	height: 0;
	border-top:22px solid transparent; 
	border-right:22px solid #a4a4a4;
	border-bottom:22px solid transparent;
	border-left:22px solid transparent;
	position: absolute;
	left: -44px;
	top: 0;
	transition: all ease 0.3s;
}

.faq-list ol li a:hover,
.faq-list ol li a:focus{
	text-decoration: none;
	background: #72ca36;
	color: #fff;
}

.faq-list ol li a:hover::before{
	border-right-color: #72ca36;
}

.faq-list ol li::before{
	content: '1';
	display: block;
	width: 44px;
	height: 44px;
	border-radius: 44px;
	background: #79797b;
	color: #fff;
	position: absolute;
	left: 0;
	top: 10px;
	text-align: center;
	line-height: 44px;
	z-index: 99;
	transition: all ease 0.3s;
	font-size: 14px;
}

.faq-list ol li:hover::before{
	background: #72ca36;
	color: #fff;
}

.faq-list ol li::after{
	content: '';
	width: 0;
	height: 30px;
	border:1px solid #72ca36;
	position: absolute;
	left:20px;
	top: 80%;
	z-index: 90;
}

.faq-list ol li:last-child::after{
	border:0 !important;
}

.faq-list ol li:nth-child(2)::before{
	content: '2';
}

.faq-list ol li:nth-child(3)::before{
	content: '3';
}

.faq-list ol li:nth-child(4)::before{
	content: '4';
}

.faq-list ol li:nth-child(5)::before{
	content: '5';
}

.faq-list ol li:nth-child(6)::before{
	content: '6';
}

.faq-list ol li:nth-child(7)::before{
	content: '7';
}

.faq-list ol li:nth-child(8)::before{
	content: '8';
}

.two-col-form .frm-control{
	padding-top: 5px;
}

.two-col-form .frm-control:nth-child(1),
.two-col-form .frm-control:nth-child(2){
	padding-top: 10px;
}
.two-col-form label.frm-label{
	font-weight: 400;
	color: #222;
	text-indent: 15px;
	position: relative;
	pointer-events: none;
	transition: all ease 0.3s;
	-webkit-transform: translateY(-28px);
	-moz-transform: translateY(-28px);
	transform: translateY(-28px);
}

.two-col-form span.required{
	position: relative;
	float: right;
	display: inline-block;
	width: 20px;
	height: 20px;
	text-align: center;
	line-height: 20px;
	font-size: 18px;
	color: red;
	-webkit-transform: translateY(-25px);
	-moz-transform: translateY(-25px);
	transform: translateY(-25px);
	pointer-events: none;
	padding-right: 10px;
}

.two-col-form input[type="text"],
.two-col-form input[type="email"],
.two-col-form input[type="password"]{
	width: 100%;
	padding: 9px 20px 9px 15px;
	background: #fff;
	border-radius: 2px;
	border:1px solid #ccc;
	transition: all ease 0.3s;
}

.two-col-form input[type="text"]:focus,
.two-col-form input[type="password"]:focus{
	-webkit-box-shadow: 0px 0px 7px 0px rgba(0,0,0,0.25);
	-moz-box-shadow: 0px 0px 7px 0px rgba(0,0,0,0.25);
	box-shadow: 0px 0px 7px 0px rgba(0,0,0,0.25);
	outline: none;
}

.two-col-form input[type="text"]:focus + label.frm-label,
.two-col-form input[type="text"]:active + label.frm-label,
.two-col-form .filled + label.frm-label{
	-webkit-transform: translateY(-55px);
	-moz-transform: translateY(-55px);
	transform: translateY(-55px);
	text-indent: 5px;
	font-weight: 600;
}

.two-col-form input[type="password"]:focus + label.frm-label,
.two-col-form input[type="password"]:active + label.frm-label,
.two-col-form .filled + label.frm-label{
	-webkit-transform: translateY(-55px);
	-moz-transform: translateY(-55px);
	transform: translateY(-55px);
	text-indent: 5px;
	font-weight: 600;
}

