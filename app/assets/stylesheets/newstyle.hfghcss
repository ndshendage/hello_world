}

.dash-circle{
	position: relative;
	width: 420px;
	height: 420px;
	border-radius: 420px;
	border:10px solid #fff;
	margin: 50px auto 20px;
	-webkit-box-shadow: 0px 0px 15px 0px rgba(0,0,0,0.25);
	-moz-box-shadow: 0px 0px 15px 0px rgba(0,0,0,0.25);
	box-shadow: 0px 0px 15px 0px rgba(0,0,0,0.25);
	background: -moz-linear-gradient(top,  rgba(125,185,232,0) 0%, rgba(202,202,202,1) 100%); /* FF3.6-15 */
	background: -webkit-linear-gradient(top,  rgba(125,185,232,0) 0%,rgba(202,202,202,1) 100%); /* Chrome10-25,Safari5.1-6 */
	background: linear-gradient(to bottom,  rgba(125,185,232,0) 0%,rgba(202,202,202,1) 100%); /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#007db9e8', endColorstr='#cacaca',GradientType=0 ); /* IE6-9 */

}

.dash-circle .main-circle{
	width: 194px;
	height: 194px;
	border-radius: 194px;
	position: absolute;
	background: #71cb35;
	color: #fff;
	border:10px solid #fff;
	-webkit-box-shadow: 0px 0px 2px 0px #369300;
	-moz-box-shadow: 0px 0px 2px 0px #369300;
	box-shadow: 0px 0px 2px 0px #369300;
	left: 0;
	top: 0;
	bottom: 0;
	right: 0;
	margin:auto;
}

.dash-circle .inner-circles{
	width: 110px;
	height: 110px;
	border-radius: 110px;
	background: #71cb35;
	color: #fff;
	border:10px solid #fff;
	-webkit-box-shadow: 0px 0px 2px 0px #369300;
	-moz-box-shadow: 0px 0px 2px 0px #369300;
	box-shadow: 0px 0px 2px 0px #369300;
	transition: background ease 0.3s;
	position: absolute;
	-webkit-transform: translateX(-55px) translateY(-55px);
	-moz-transform: translateX(-55px) translateY(-55px);
	transform: translateX(-55px) translateY(-55px);
}

.dash-circle .inner-circles a{
	display: block;
	width: 100%;
	color: #fff;
	font-size: 14px;
	text-align: center;
	padding-top: 22px;
}

.inner-circles:hover,
.inner-circles:focus{
	background: #79797b;
}

.dash-circle .inner-circles a:hover,
.dash-circle .inner-circles a:focus{
	text-decoration: none;
}

.dash-circle .inner-circles.centered{
	left: 50%;
	top: 0;
}

.dash-circle .inner-circles.lower-left{
	left: 0;
	top: 67%;
}

.dash-circle .inner-circles.lower-right{
	right: 0;
	top: 67%;
	-webkit-transform: translateX(55px) translateY(-55px);
	-moz-transform: translateX(55px) translateY(-55px);
	transform: translateX(55px) translateY(-55px);
}

