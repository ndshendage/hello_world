.navbar-default{
	background-color: #71cb35;
	border:0;
	border-radius: 0;
}

.navbar-collapse{
	padding: 0;
}

.main-nav a{
	font-size: 14px;
	color: #fff !important;
	font-weight: 400;
	position: relative;
	overflow: hidden;
}

.main-nav a:before{
	content: attr(data-title);
	display: inline-block;
	position: absolute;
	left:5px;
	top: 5px;
	padding: 10px;
	font-size: 14px;
	color: #fff !important;
	font-weight: 400;
	background: #79797b;
	width: auto;
	height: 80%;
	margin:0;	
	text-align: left;
	transform: translateY(-45px);
	transition: all ease-in 0.2s;
	border-radius: 2px;
}

.main-nav a.home:before{
	content: url('dashboard-icon.png');
	transform: translateY(-48px);
}

.main-nav li.active a:before,
.main-nav li a:hover::before{
	transform: translateY(0);
}

.main-nav >.active>a, 
.main-nav >.active>a:focus, 
.main-nav >.active>a:hover,
.navbar-default .navbar-nav>.active>a, 
.navbar-default .navbar-nav>.active>a:focus, 
.navbar-default .navbar-nav>.active>a:hover{
	background: none;
}

