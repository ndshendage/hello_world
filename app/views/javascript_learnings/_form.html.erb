
<div class="modal-dialog form">
  <div class="modal-content">
    <%= form_for(@javascript_learning) do |f| %>
    <div class="modal-body">
      <% if @javascript_learning.errors.any? %>
        <div id="error_explanation">
          <h2><%= pluralize(@javascript_learning.errors.count, "error") %> prohibited this javascript_learning from being saved:</h2>

          <ul>
          <% @javascript_learning.errors.full_messages.each do |msg| %>
            <li><%= msg %></li>
          <% end %>
          </ul>
        </div>
      <% end %>

      <div class="form-group">
        <%= f.label :function_name, class:"control-label" %>
        <%= f.text_field :function_name, class: "form-control" %>
      </div>
      <div class="form-group">
        <%= f.label :description, class:"control-label"%>
        <%= f.text_area :description, class: "form-control" %>
      </div>

      <div class="form-group">
        <%= f.label :minimum, class:"control-label"%>
        <%= f.number_field :minimum, class: "form-control" %>
        <%#= f.select :minimum, options_for_select(@countries.collect { |country|
    [country.name.titleize, country.address] }, 7), {}, {class: "form-control" }%>
         <%#= f.select :minimum, options_for_select(@countries)%>
      </div>

      <div class="form-group">
        <%= f.label :maximum, class:"control-label"%>
        <%= f.number_field :maximum, class: "form-control" %>
      </div>

      <div class="modal-footer ">
        <%= f.submit class: "btn btn-success text-center left" %>
        <%= link_to "Cancel", "/javascript_learnings", class: "btn btn-default text-center left", data: {dismiss: "modal"} %>
      </div >
    </div>
    <% end %>
  </div>
</div>

<script type="text/javascript">
  $(document).on('change', "", function(){
    val = $(this).val()
    alert("input changed"+);
  })
</script>